{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.profiler", {token: token}, {fragment: 'cache'}) }}">
        <img width="20" height="28" alt="Cache"
             src="data:image/png;base64,"/>
        <span>{{ collector.countcachecids }}</span>
    </a>
    {% endset %}
    {% set text %}

    {% for bin, cids in collector.cachecids %}
        <div class="sf-toolbar-info-piece">
            <b>{{ bin }}</b>
            <span>{{ cids|length }}</span>
        </div>
    {% endfor %}
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block menu %}
    {{ 'Cache'|t }}
{% endblock %}

{% block menu_summary %}
    {{ 'Total cache'|t }}: {{ collector.countcachecids }}
{% endblock %}

{% block panel %}
    <h2>{{ 'Cache'|t }}</h2>
    {% for bin, cids in collector.cachecids %}
        <details>
            <summary>{{ bin }} ({{ cids|length }})</summary>
            {% include '@webprofiler/Profiler/table.html.twig' with { 'data': cids, 'class': 'inline' } only %}
        </details>
    {% endfor %}
{% endblock %}
