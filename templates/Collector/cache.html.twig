{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.dashboard", {profile: token}, {fragment: 'cache'}) }}" title="{{ 'Cache'|t }}">
        <img width="20" height="28" alt="{{ 'Cache'|t }}"
             src="data:image/png;base64,{{ collector.icon }}"/>
        <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.countcacheHits }}
            / {{ collector.countcacheMisses }}</span>
    </a>
    {% endset %}
    {% set text %}

    {% for bin, cids in collector.cacheHits %}
        <div class="sf-toolbar-info-piece">
            <b>{{ bin }}</b>
            <span>{{ cids }}</span>
        </div>
    {% endfor %}
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block panel %}
    <script id="cache" type="text/template">
        <h2 class="panel__title">{{ 'Cache'|t }}</h2>
        <div class="panel__container">

        </div>
    </script>
{% endblock %}
