{% block toolbar %}
    {% set icon %}
    <a href="{{ url("webprofiler.profiler", {token: token}, {fragment: 'state'}) }}">
        <img width="20" height="28" alt="Database"
             src="data:image/png;base64,"/>
        <span>{{ collector.statekeys|length }}</span>
    </a>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>{{ 'Get'|t }}</b>
        <span>{{ collector.statekeys|length }}</span>
    </div>
    {% endset %}

    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
        <div class="sf-toolbar-info">{{ text|default('') }}</div>
    </div>
{% endblock %}

{% block menu %}
    {{ 'State'|t }}
{% endblock %}

{% block menu_summary %}
    {{ 'State variabiles'|t }}: {{ collector.statekeys|length }}
{% endblock %}

{% block panel %}
    <h2>State</h2>
    <details>
        <summary>{{ 'Get'|t }}</summary>
        {% include '@webprofiler/Profiler/table.html.twig' with { 'data': collector.statekeys, 'class': 'inline' } only %}
    </details>
{% endblock %}
